    "type": "object",
    "properties": {
      "export_date": {"type":"string","format":"date"},
      "transactions": {"type":"array","items":{"type":"object"}}
    },
    "required":["export_date","transactions"]
  },
  "response": {
    "200": {"type":"object","properties":{"status":{"type":"string"},"rows_appended":{"type":"integer"}}}
  }
}
